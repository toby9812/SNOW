  SET I003 40
  SETREG MREG#(1) I003
MOVL P039 V=I001 PL=1
  SET I003 39
  SETREG MREG#(1) I003
MOVL P038 V=I001 PL=1
  SET I003 38
  SETREG MREG#(1) I003
MOVL P037 V=I001 PL=1
  SET I003 37
  SETREG MREG#(1) I003
