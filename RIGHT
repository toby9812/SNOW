  SET I003 160
  SETREG MREG#(1) I003
MOVL P161 V=I001
  SET I003 161
  SETREG MREG#(1) I003
SFTOF

GETREG I002 MREG#(9)
SET D002 I002
MUL D002 1000
ADD D001 D002
IF (D001>=D014) THEN
SET D001 D014
ENDIF
SETE P169 (3) D001

SFTON P169 UF#(3)
MOVL P161 V=I001
  SET I003 161
  SETREG MREG#(1) I003
MOVL P160 V=I001 PL=1
  SET I003 160
  SETREG MREG#(1) I003
