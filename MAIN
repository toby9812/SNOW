NOP
CLEAR STACK
SET I004 1
SETREG MREG#(2) I004
IF (I003<>1) THEN
JUMP JOB:HOME
ENDIF

*LABEL
GETREG I001 MREG#(8)
SETREG MREG#(0) I001
SET D0003 I0001
DIV D0003 2
GETREG I002 MREG#(9)

IF ( IN#(1)=ON ) THEN
SET I004 0
SETREG MREG#(2) I004
JUMP JOB:HOME
ENDIF
IF ( IN#(10)=ON ) THEN
SET I004 0
SETREG MREG#(2) I004
JUMP JOB:LEFT
ENDIF
IF ( IN#(11)=ON ) THEN
SET I004 0
SETREG MREG#(2) I004
JUMP JOB:COVER
ENDIF


JUMP *LABEL
END
