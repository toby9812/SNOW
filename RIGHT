  SET I003 155
  SETREG MREG#(1) I003
MOVL P154 V=I001 PL=1
  SET I003 154
  SETREG MREG#(1) I003
MOVL P153 V=I001 PL=1
  SET I003 153
  SETREG MREG#(1) I003
MOVL P152 V=I001 PL=1
  SET I003 152
  SETREG MREG#(1) I003
MOVL P151 V=I001
  SET I003 151
  SETREG MREG#(1) I003
SFTOF

IF (D001>=D014) THEN
JUMP *ZOOM2
ENDIF
GETREG I002 MREG#(9)
SET D002 I002
MUL D002 1000
ADD D001 D002
IF (D001>=D014) THEN
SET D001 D014
ENDIF
SETE P169 (3) D001
JUMP *ZOOM1

*ZOOM2
SFTOF
MOVL P171 V=I001
  SET I003 171
  SETREG MREG#(1) I003
