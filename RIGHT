SET D003 I001
DIV D003 7

CNVRT PX169 PX169 UF#(3)
SET D001 0
SETE P169 (1) 0
SETE P169 (2) 0
SETE P169 (3) 0
SETE P169 (4) 0
SETE P169 (5) 0
SETE P169 (6) 0
GETE D014 P168(3)
GETE D015 P157(3)
SUB D014 D015

*ZOOM1
SFTON P169 UF#(3)
MOVL P151 V=I001
  SET I003 151
  SETREG MREG#(1) I003
